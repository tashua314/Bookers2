<div class="card-list-containar py-5">
  <% users.each do |user| %>
  <div class="card w-50 mx-auto mt-4 shadow-lg">
    <div class="card-body">
      <div class="row">
        <div class="col-2">
          <%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg", class:"mt-3 rounded-circle"; %>
        </div>
      </div>
    </div>
  </div>
  <% end %>
  <%= paginate users %>
</div>
